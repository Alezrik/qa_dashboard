<h1>Show User ingest token</h1>

<ul>

  <li>
    <strong>Token:</strong>
    <%= @user_ingest_token.token %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @user_ingest_token.name %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @user_ingest_token.type %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_ingest_token_path(@conn, :edit, @user_ingest_token) %></span>
<span><%= link "Back", to: Routes.user_ingest_token_path(@conn, :index) %></span>
